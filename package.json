{
  "name": "ultimate-next-starter",
  "description": "The ultimate starter for a new next project, powered by Apollo with graphql-let for major quality of life improvements",
  "version": "1.0.0",
  "private": true,
  "author": "Daniel Cooke <danielcooke1996@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "apollo",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@material-ui/core": "4.11.3",
    "apollo-server-micro": "2.21.0",
    "graphql": "15.5.0",
    "graphql-tools": "7.0.4",
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/plugin-helpers": "1.18.2",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql-let": "0.16.3",
    "husky": "5.1.1",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.2",
    "yaml-loader": "0.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
